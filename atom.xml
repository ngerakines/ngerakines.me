---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 <title>ngerakines.me</title>
 <link href="http://www.ngerakines.me/atom.xml" rel="self"/>
 <link href="http://ngerakines.me/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://ngerakines.me/</id>
 <author>
   <name>Nick Gerakines</name>
 </author>
 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://www.ngerakines.me{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://www.ngerakines.me{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
</feed>